# Trua Collect Environment Configuration

# Default collection key to use when no key is provided in URL
# Format: [language]-[personal]-[consents]-[residence]-[employment]-[education]-[proLicense]-[signature]
# Example: en-EPMA-DTB-R5-E5-E-P-W  - English, Email+Phone+Name+Alias, Driver+Test+Bio consents, 5-year residence, 5-year employment, Education enabled, Pro licenses enabled, Wet signature
VITE_DEFAULT_COLLECTION_KEY=en-N-N-R3-N-N-N-W
 
# Server port
VITE_PORT=3000

# Development mode (true/false)
VITE_DEV_MODE=false

# Log level (debug, info, warn, error)
VITE_LOG_LEVEL=debug

# API endpoints
VITE_API_BASE_URL=http://localhost:3001/api

# Document storage path
VITE_DOCUMENT_STORAGE_PATH=./storage/documents